-------------------------------------------------------------------Brute Force Approach -----------------------------------------------------------------------------------------
-----------------------------------------------------------------Time complexity : O(n*m)----------------------------------------------------------------------------------------

class Solution:
    def removeOccurrences(self, s: str, part: str) -> str:
        stack = []
        w = len(part)
        i = 0 
        while i< len(s):
            if s[i]==part[0] and s[i:w] == part:
                i+=w
            else:
                stack.append(s[i])
        return ''.join(stack)
